{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "JWT": {
    "Issuer": "altasoft",
    "Audience": "any",
    "ExpiresInSeconds": 300000,
    "SecretKey": "1234567890123456",
    "Cookie": {
      "Name": "JWTAccessToken",
      "AuthenticationScheme": "Cookie",
      "Domain": "",
      "Path": "",
      "ExpiresInSeconds": 30,
      "SlidingExpirationInSeconds": 5,
      "HttpOnly": true,

      "LoginPath": "/api/account/login",
      "LogoutPath": "/api/account/logout",
      "AccessDeniedPath": null
    }
  }
}
